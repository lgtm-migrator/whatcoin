{
  "env": {
    "commonjs": true,
    "es2020": true,
    "node": true
  },
  "plugins": ["jest"],
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended",
    "plugin:jest/style"
  ],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "linebreak-style": ["error", "unix"]
  }
}
